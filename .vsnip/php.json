{
  "Loop": {
    "prefix": "loop",
    "body": [
      "<?php if (have_posts()): ?>",
      "\t<?php while (have_posts()): ?>",
      "\t\t<?php the_post();?>",
      "\t\t<?php endwhile;?>",
      "\t<?php wp_reset_postdata();?>",
      "<?php endif;?>"
    ],
    "description": "loop"
  },
  "Looph": {
    "prefix": "looph",
    "body": [
      "<?php if ($$1->have_posts()): ?>",
      "\t<?php while ($$1->have_posts()): ?>",
      "\t\t<?php $$1->the_post();?>",
      "\t\t<?php endwhile;?>",
      "\t<?php wp_reset_postdata();?>",
      "<?php endif;?>"
    ],
    "description": "looph"
  },
